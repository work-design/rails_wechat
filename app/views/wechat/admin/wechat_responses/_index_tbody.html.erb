<td>
  <%= model.match_value %>
  <%= image_tag model.qrcode_file, class: 'image is-32x32' if model.qrcode_file.attached? %>
</td>
<td>
  <div class="ui toggle checkbox">
    <%= check_box_tag '[model]contain', nil, model.contain, data: { method: :patch, url: admin_wechat_app_wechat_response_path(@wechat_app, model), submit: true }, id: nil %>
  </div>
</td>
<td>
  <p><time data-controller="time"><%= model.created_at.to_s(:rfc822) %></time></p>
  <p><time data-controller="time"><%= model.expire_at.to_s(:rfc822) if model.expire_at %></time></p>
</td>
<td>
  <% model.wechat_response_requests.each do |wechat_response_request| %>
    <p><%= Wechat::WechatRequest.enum_i18n(:type, wechat_response_request.request_type) %></p>
  <% end %>
</td>
<td>
  <p>
    <span><%= model.effective_type %></span>
    <%= link_to({ action: 'edit_reply', id: model.id }, aria: { label: t('.edit') }, class: 'button is-small is-rounded is-light') do %>
      <i class="fas fa-pencil-alt"></i>
    <% end %>
  </p>
  <p><%= model.effective_id %></p>
</td>
