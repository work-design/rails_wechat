<%= render 'shared/error_messages', target: @wechat_response %>
<%= f.select :request_type, options_for_select(WechatRequest.options_i18n(:type), @wechat_response.request_type) %>
<%= f.check_box :contain %>
<%= f.text_field :match_value %>
<%= f.select :effective_type, options_for_select(WechatReply.options_i18n(:type), @wechat_response.effective_type) %>
<%= f.collection_select :wechat_extractor_ids, @wechat_extractors, :id, :name, {}, { multiple: true, data: { controller: 'choice' } } %>
<%= f.submit %>
