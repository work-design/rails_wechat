<%= render 'shared/error_messages', target: @wechat_app %>
<%= f.select :type, options_for_select(WechatApp.options_i18n(:type), @wechat_app.type) %>
<%= f.text_field :name %>
<%= f.text_field :appid %>
<%= f.text_field :secret %>
<%= f.check_box :encrypt_mode %>
<h4 class="ui dividing center aligned header">微信支付</h4>
<%= f.text_field :mch_id %>
<%= f.text_field :key %>
<h4 class="ui dividing center aligned header">企业微信</h4>
<%= f.text_field :agentid %>
<h4 class="ui dividing center aligned header">其他</h4>
<%= f.submit %>
