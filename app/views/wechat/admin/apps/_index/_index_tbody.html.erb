<td>
  <p><%= model.name %></p>
  <p><span class="tag is-info"><%= model.type_i18n %></span></p>
</td>
<td>
  <p><%= model.appid %></p>
  <%= content_tag :dl do %>
    <dt data-show-target="item" class="has-text-grey" style="visibility: hidden"><%= Wechat::App.human_attribute_name(:agentid) %></dt>
    <dd><%= model.agentid %></dd>
  <% end if model.agentid.present? %>
</td>
<td>
  <%= form_with theme: nil, model: model, scope: :app, url: { action: 'update', id: model.id }, data: { controller: 'input' } do |f| %>
    <%= f.check_box :encrypt_mode, data: { input_target: 'checkbox' }, id: nil, class: 'switch is-rounded' %>
    <label data-action="click->input#check"></label>
  <% end %>
</td>
<td>
  <%= form_with theme: nil, model: model, scope: :app, url: { action: 'update', id: model.id }, data: { controller: 'input' } do |f| %>
    <%= f.check_box :enabled, data: { input_target: 'checkbox' }, id: nil, class: 'switch is-rounded' %>
    <label data-action="click->input#check"></label>
  <% end %>
  <%= form_with theme: nil, model: model, scope: :app, url: { action: 'update', id: model.id }, data: { controller: 'input' } do |f| %>
    <%= f.check_box :inviting, data: { input_target: 'checkbox' }, id: nil, class: 'switch is-rounded' %>
    <label data-action="click->input#check"></label>
  <% end %>
</td>
<td>
  <p><%= model.domain %></p>
  <p><%= model.weapp_id %></p>
</td>
