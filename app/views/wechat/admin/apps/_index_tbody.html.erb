<td>
  <p><%= model.name %></p>
  <p><span class="tag is-info"><%= model.type_i18n %></span></p>
</td>
<td>
  <p><%= model.appid %></p>
</td>
<td>
  <%= form_with theme: 'switch', model: model, scope: :app, url: { action: 'update', appid: model.appid }, data: { controller: 'input' } do |f| %>
    <%= f.check_box :encrypt_mode, data: { action: 'input#submit' }, id: nil %>
  <% end %>
</td>
<td>
  <%= form_with theme: 'switch', model: model, scope: :app, url: { action: 'update', appid: model.appid }, data: { controller: 'input' } do |f| %>
    <%= f.check_box :enabled, data: { action: 'input#submit' }, id: nil %>
  <% end %>
</td>
<td>
  <p><%= model.domain %></p>
  <p><%# model.weapp_id %></p>
</td>
