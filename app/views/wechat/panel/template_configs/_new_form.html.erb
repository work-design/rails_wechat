<%= form_with model: @template_config, url: panel_template_configs_path do |f| %>
  <%= render 'shared/error_messages', target: @template_config %>
  <%= f.select :type, options_for_select(TemplateConfig.options_i18n(:type)) %>
  <%= f.text_field :tid %>
  <%= f.text_field :description %>
  <%= f.submit %>
<% end %>
