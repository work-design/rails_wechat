<td><%= model.name %></td>
<td>
  <p><%= model.appid %></p>
  <p><%= model.secret %></p>
  <p><%= model.token %></p>
  <p><%= model.encoding_aes_key %></p>
  <p><%= model.verify_ticket %></p>
  <p><code><%= model.access_token %></code></p>
  <p><time data-controller="time"><%= model.access_token_expires_at&.to_fs(:iso8601) %></time></p>
  <p><%= model.pre_auth_code %></p>
  <p><time data-controller="time"><%= model.pre_auth_code_expires_at&.to_fs(:iso8601) %></time></p>
</td>
