<td>
  <div class="is-several">
    <%= image_tag model.avatar_url, class: 'image is-24x24' if model.avatar_url.present? %>
    <span><%= model.name %></span>
  </div>
  <p>
    <span class="tag"><%= model.type_i18n %></span>
    <span><%= model.app&.name if model.respond_to?(:app) %></span>
  </p>
  <p><%= model.appid %></p>
</td>
<td>
  <p class="has-text-success"><%= model.uid %></p>
  <p class="has-text-grey"><%= model.unionid %></p>
</td>
<td>
  <p class="has-text-grey"><%= model.identity %></p>
  <p class="has-text-success"><%= model.external_userid %></p>
  <p class="has-text-info"><%= model.pending_id %></p>
</td>
<td><time data-controller="time"><%= model.created_at.to_fs(:iso8601) %></time></td>
