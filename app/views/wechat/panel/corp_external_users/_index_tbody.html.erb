<td>
  <p>
    <span><%= model.wechat_user.app.name %></span>
    <span class="tag"><%= model.wechat_user.app.type_i18n %></span>
  </p>
  <p><%= model.corp.name %></p>
</td>
<td>
  <div class="is-several">
    <%= image_tag model.wechat_user.avatar_url, class: 'image is-24x24' if model.wechat_user.avatar_url.present? %>
    <span><%= model.wechat_user.name %></span>
  </div>
  <p class="text-success"><%= model.uid %></p>
  <p class="text-grey"><%= model.unionid %></p>
</td>
<td>
  <p><%= model.subject_type_i18n %></p>
  <p class="text-success"><%= model.external_userid %></p>
  <p><%= model.pending_id %></p>
</td>
<td><time data-controller="time"><%= model.created_at.to_fs(:iso8601) %></time></td>
<td>
  <div data-show-target="item" class="visibility-hidden">
    <%= link_to({ action: 'show', id: model.id }, data: { turbo_frame: 'modal' }, aria: { label: t('.show.title') }, class: 'button is-small is-borderless') do %>
      <%= svg_tag 'circle-info', class: 'icon text-info' %>
    <% end %>
    <%= link_to({ action: 'edit', id: model.id }, data: { turbo_frame: 'modal' }, aria: { label: t('.show.title') }, class: 'button is-small is-borderless') do %>
      <%= svg_tag 'pencil', class: 'icon text-link' %>
    <% end %>
  </div>
</td>