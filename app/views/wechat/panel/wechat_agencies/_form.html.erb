<%= form_with model: @wechat_agency do |f| %>
  <%= render 'shared/error_messages', target: @wechat_agency %>
  <%= f.text_field :appid %>
  <%= f.text_field :access_token %>
  <%= f.text_field :access_token_expires_at %>
  <%= f.text_field :refresh_token %>
  <%= f.text_field :func_infos %>
  <%= f.submit %>
<% end %>
