<td>
  <p><%= model.userid %></p>
  <p class="text-grey"><time data-controller="time"><%= model.created_at.to_fs(:iso8601) %></time></p>
</td>
<td>
  <p><%= model.device_id %></p>
  <p><%= model.open_userid %></p>
</td>
<td><%= model.member&.id %></td>
<td>
  <div data-show-target="item" class="visibility-hidden">
    <%= link_to({ controller: 'externals', userid: model.userid }, aria: { label: t('wechat.panel.externals.index.title') }, class: 'button is-small is-rounded is-light') do %>
      <i class="fa-solid fa-users-line text-info"></i>
    <% end %>
    <%= link_to({ action: 'show', id: model.id }, data: { turbo_frame: 'modal' }, aria: { label: t('.show.title') }, class: 'button is-small is-rounded is-light') do %>
      <i class="fa-solid fa-circle-info text-info"></i>
    <% end %>
    <%= link_to({ action: 'edit', id: model.id }, data: { turbo_frame: 'modal' }, aria: { label: t('.edit.title') }, class: 'button is-small is-rounded is-light') do %>
      <i class="fa-solid fa-pencil text-link"></i>
    <% end %>
    <%= button_to({ action: 'destroy', id: model.id }, method: :delete, aria: { label: t('.destroy.title') }, data: { turbo_confirm: t('.destroy.confirm') }, class: 'button is-small is-rounded is-light') do %>
      <i class="fa-solid fa-trash text-danger"></i>
    <% end %>
  </div>
</td>