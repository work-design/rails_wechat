<div class="panel-block" data-action="click->typer#choose" data-id="<%= model.id %>" data-name="<%= model.name %>">
  <span><%= image_tag model.head_img, class: 'image is-16x16' if model.head_img.present? %></span>
  <span><%= model.nick_name %></span>
  <span class="has-text-grey">(<%= model.id %>)</span>
</div>
