<td>
  <i class="fa-solid fa-qrcode"></i>
  <span class="pl-1"><%= model.match_value %></span>
</td>
<td>
  <p><time data-controller="time"><%= model.created_at.to_fs(:iso8601) %></time></p>
  <p><time data-controller="time" class="<%= model.expire_at && model.expire_at < Time.current ? 'text-danger' : 'text-success' %>"><%= model.expire_at&.to_fs(:iso8601) %></time></p>
</td>
<td></td>
<td>
  <%= link_to({ controller: 'wechat_users', scene_id: model.id }, aria: { label: t('wechat.in.wechat_users.index.title') }, class: 'button is-small is-borderless') do %>
    <%= svg_tag 'users' %>
    <span class="ml-1"><%= model.tag.user_tags.size %></span>
  <% end if model.tag %>
</td>
